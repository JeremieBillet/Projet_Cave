package com.intiformation.projet.caveavin.modele;

public class Categorie {
	
	private int id;
	private String classe;
	private String nom;
	private String photo;
	
	
	public Categorie(int id, String classe, String nom, String photo) {
		super();
		this.id = id;
		this.classe = classe;
		this.nom = nom;
		this.photo = photo;
	}


	public Categorie(String classe, String nom, String photo) {
		super();
		this.classe = classe;
		this.nom = nom;
		this.photo = photo;
	}


	public Categorie(int id, String classe, String nom) {
		super();
		this.id = id;
		this.classe = classe;
		this.nom = nom;
	}


	public Categorie(String classe, String nom) {
		super();
		this.classe = classe;
		this.nom = nom;
	}


	public int getId() {
		return id;
	}


	public void setId(int id) {
		this.id = id;
	}


	public String getClasse() {
		return classe;
	}


	public void setClasse(String classe) {
		this.classe = classe;
	}


	public String getNom() {
		return nom;
	}


	public void setNom(String nom) {
		this.nom = nom;
	}


	public String getPhoto() {
		return photo;
	}


	public void setPhoto(String photo) {
		this.photo = photo;
	}
	
	

}
